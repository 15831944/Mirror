
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script>
        function CheckJWT() {
            var url = "http://localhost:5000/api/SMS/CheckJWTStr";
            var token = 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..Nb-iOQZF0fVlFDY_hcLUnr83LAypa3eOtz6rBpBP__I';
            $.ajax({
                type: "post",
                url: url,
                //contentType: "application/json",
                data: { "content": "123" },
                dataType: "text",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("Authorization", token);
                },
                success: function (data) {
                    alert(data);
                }, error: function (error) {
                    console.log(error);
                }
            });
        }
        function CheckTest() {
            var url = "http://localhost:5000/api/SMS/CheckTest";
            $.ajax({
                type: "post",
                url: url,
                beforeSend: function (XMLHttpRequest) {
                },
                success: function (data) {
                    alert(data);
                }, error: function (error) {
                    console.log(error);
                }
            });
        }
    </script>

</head>
<body>
    <div>
        <input type="button" name="name" value="验证签名" onclick="CheckJWT()" />
        <input type="button" name="name" value="测试签名" onclick="CheckTest()" />
    </div>
</body>
</html>
